{
  "name": "ex-site-client",
  "version": "1.0.0",
  "description": "Node environment testing for the client-side",
  "private": false,
  "homepage": "https://github.com/A0x1E/ex-site-client#readme",
  "keywords": [
    "site",
    "vue",
    "tailwindcss"
  ],
  "author": "Sergey Maslakov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/A0x1E/ex-site-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/A0x1E/ex-site-client.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier . --write",
    "lint:eslint": "eslint --ext .{js,ts,vue} . --fix",
    "lint:style": "stylelint 'src/**/*.vue'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "stylelint": "^14.5.1",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vitest": "^0.4.3",
    "vue-tsc": "^0.31.4"
  },
  "dependencies": {
    "vue": "^3.2.31",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  }
